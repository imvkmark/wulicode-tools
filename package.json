{
    "name": "wulicode-webapp",
    "version": "0.0.1",
    "private": true,
    "description": "wulicode webapp",
    "scripts": {
        "start:local": "vite --mode=self-serve",
        "start:prod": "vite --mode=prod-serve",
        "build:local": "vite build --mode=self",
        "build:prod": "vite build --mode=prod",
        "build-tsc": "vue-tsc --noEmit && vite build",
        "serve": "vite preview",
        "format": "prettier --write ./src",
        "lint": "eslint ./src --ext .vue,.js,.ts",
        "lint-fix": "eslint --fix ./src --ext .vue,.js,.ts"
    },
    "keywords": [
        "Vite",
        "Vue3",
        "TypeScript",
        "Starter"
    ],
    "web-types": "src/utils/types/web-types.json",
    "author": "duoli",
    "license": "MIT",
    "lint-staged": {
        "*.{vue,js,ts}": "eslint --fix"
    },
    "dependencies": {
        "@ant-design/icons-vue": "^6.0.1",
        "@popjs/util": "^0.3.1",
        "@sentry/tracing": "^6.13.3",
        "@sentry/vue": "^6.13.3",
        "@vant/use": "^1.3.2",
        "axios": "^0.22.0",
        "crypto-js": "^4.0.0",
        "dayjs": "^1.10.6",
        "dotenv": "^10.0.0",
        "element-plus": "^1.1.0-beta.19",
        "lodash-es": "^4.17.21",
        "mitt": "^3.0.0",
        "vue": "^3.0.5",
        "vue-router": "^4.0.6",
        "vue3-clipboard": "^1.0.0",
        "vuex": "^4.0.0"
    },
    "devDependencies": {
        "@types/crypto-js": "^4.0.2",
        "@types/lodash-es": "^4.17.4",
        "@types/node": "^16.10.3",
        "@typescript-eslint/eslint-plugin": "^4.33.0",
        "@typescript-eslint/parser": "^4.33.0",
        "@vitejs/plugin-vue": "^1.9.3",
        "@vue/compiler-sfc": "^3.2.20",
        "eslint": "^7.24.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-vue": "^7.19.1",
        "less": "^4.1.2",
        "typescript": "^4.1.3",
        "vite": "^2.6.5",
        "vite-plugin-sentry": "^1.0.9",
        "vue-tsc": "^0.28.1"
    }
}
