{
    "name": "wulicode-webapp",
    "version": "0.0.1",
    "private": true,
    "description": "wulicode webapp",
    "scripts": {
        "start:local": "vite --mode=self-serve",
        "start:prod": "vite --mode=prod-serve",
        "build:local": "vite build --mode=self",
        "build:prod": "vite build --mode=prod",
        "build-tsc": "vue-tsc --noEmit && vite build",
        "serve": "vite preview",
        "test": "jest --config ./jest.config.js",
        "format": "prettier --write ./src",
        "lint": "eslint ./src --ext .vue,.js,.ts",
        "lint-fix": "eslint --fix ./src --ext .vue,.js,.ts",
        "prepare": "husky install config/.husky"
    },
    "keywords": [
        "Vite",
        "Vue3",
        "TypeScript",
        "Starter"
    ],
    "web-types": "src/utils/types/web-types.json",
    "author": "duoli",
    "license": "MIT",
    "lint-staged": {
        "*.{vue,js,ts}": "eslint --fix"
    },
    "dependencies": {
        "@element-plus/icons": "^0.0.11",
        "@sentry/tracing": "^6.18.1",
        "@sentry/vue": "^6.18.1",
        "@types/jest": "^27.4.1",
        "@vant/use": "^1.3.4",
        "axios": "^0.26.0",
        "crypto-js": "^4.0.0",
        "dayjs": "^1.10.8",
        "element-plus": "^2.0.5",
        "keymaster": "^1.6.2",
        "lodash-es": "^4.17.21",
        "mavon-editor": "^3.0.0-beta",
        "mitt": "^3.0.0",
        "qs": "^6.10.2",
        "ua-parser-js": "^1.0.2",
        "vue": "^3.2.31",
        "vue-router": "^4.0.13",
        "vue3-clipboard": "^1.0.0",
        "vuex": "^4.0.0"
    },
    "devDependencies": {
        "@commitlint/cli": "^16.2.1",
        "@commitlint/config-conventional": "^16.2.1",
        "@types/crypto-js": "^4.1.1",
        "@types/keymaster": "^1.6.30",
        "@types/lodash-es": "^4.17.6",
        "@types/node": "^17.0.21",
        "@types/qs": "^6.9.7",
        "@types/ua-parser-js": "^0.7.36",
        "@typescript-eslint/eslint-plugin": "^5.13.0",
        "@typescript-eslint/parser": "^5.13.0",
        "@vitejs/plugin-vue": "^2.2.4",
        "@vue/compiler-sfc": "^3.2.31",
        "dotenv": "^16.0.0",
        "eslint": "^8.10.0",
        "eslint-plugin-import": "^2.25.3",
        "eslint-plugin-vue": "^8.5.0",
        "husky": "^7.0.4",
        "jest": "^27.5.1",
        "less": "^4.1.2",
        "ts-jest": "^27.1.1",
        "typescript": "^4.6.2",
        "vite": "^2.8.6",
        "vite-plugin-sentry": "^1.0.13",
        "vue-tsc": "^0.32.1"
    }
}
