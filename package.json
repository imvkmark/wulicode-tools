{
    "name": "wulicode-webapp",
    "version": "0.0.1",
    "private": true,
    "description": "wulicode webapp",
    "scripts": {
        "start:local": "vite --mode=self-serve",
        "start:prod": "vite --mode=prod-serve",
        "build:local": "vite build --mode=self",
        "build:prod": "vite build --mode=prod",
        "build-tsc": "vue-tsc --noEmit && vite build",
        "serve": "vite preview",
        "test": "jest --config ./jest.config.js",
        "format": "prettier --write ./src",
        "lint": "eslint ./src --ext .vue,.js,.ts",
        "lint-fix": "eslint --fix ./src --ext .vue,.js,.ts"
    },
    "keywords": [
        "Vite",
        "Vue3",
        "TypeScript",
        "Starter"
    ],
    "web-types": "src/utils/types/web-types.json",
    "author": "duoli",
    "license": "MIT",
    "lint-staged": {
        "*.{vue,js,ts}": "eslint --fix"
    },
    "dependencies": {
        "@element-plus/icons": "^0.0.11",
        "@popjs/util": "^0.3.1",
        "@sentry/tracing": "^6.15.0",
        "@sentry/vue": "^6.15.0",
        "@types/jest": "^27.0.3",
        "@vant/use": "^1.3.4",
        "axios": "^0.24.0",
        "crypto-js": "^4.0.0",
        "dayjs": "^1.10.6",
        "dotenv": "^10.0.0",
        "element-plus": "^1.2.0-beta.5",
        "lodash-es": "^4.17.21",
        "meilisearch": "^0.23.0",
        "mitt": "^3.0.0",
        "qs": "^6.10.2",
        "vue": "^3.2.8",
        "vue-router": "^4.0.1",
        "vue3-clipboard": "^1.0.0",
        "vuex": "^4.0.0"
    },
    "devDependencies": {
        "@types/crypto-js": "^4.0.2",
        "@types/lodash-es": "^4.17.4",
        "@types/node": "^16.11.11",
        "@types/qs": "^6.9.7",
        "@typescript-eslint/eslint-plugin": "^4.33.0",
        "@typescript-eslint/parser": "^4.33.0",
        "@vitejs/plugin-vue": "^1.10.1",
        "@vue/compiler-sfc": "^3.2.24",
        "eslint": "^7.24.0",
        "eslint-plugin-import": "^2.25.3",
        "eslint-plugin-vue": "^7.20.0",
        "jest": "^27.4.3",
        "less": "^4.1.2",
        "ts-jest": "^27.1.0",
        "typescript": "^4.5.2",
        "vite": "^2.6.14",
        "vite-plugin-sentry": "^1.0.12",
        "vue-tsc": "^0.29.8"
    }
}
